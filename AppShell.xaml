<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:HipHopMusic.Views"
    x:Class="HipHopMusic.AppShell">

    <ShellContent Route="inicio" ContentTemplate="{DataTemplate views:InicioPage}" />
    <ShellContent Route="artistas" ContentTemplate="{DataTemplate views:ArtistasPage}" />
    <ShellContent Route="canciones" ContentTemplate="{DataTemplate views:CancionesPage}" />
    <ShellContent Route="detallecancion" ContentTemplate="{DataTemplate views:DetalleCancionPage}" />

</Shell>
